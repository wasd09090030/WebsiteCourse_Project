{
  "name": "website-course-project",
  "version": "1.0.0",
  "description": "LightYear Robotics Website with Sass",
  "main": "index.html",
  "scripts": {
    "sass": "sass sass/main.scss style.css",
    "sass:watch": "sass --watch sass/main.scss:style.css",
    "sass:compressed": "sass sass/main.scss style.css --style compressed",
    "build": "npm run sass:compressed",
    "dev": "npm run sass:watch"
  },
  "devDependencies": {
    "sass": "^1.69.5"
  },
  "keywords": [
    "sass",
    "css",
    "website",
    "robotics"
  ],
  "author": "Your Name",
  "license": "MIT"
}
